<!-- Loop over each object in the blogposts array -->
<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <div class="container-fluid">
      <h1><span class="navbar-brand badge bg-secondary">Your Dashboard</span></h1>
  </div>
</nav>
{{#each blogposts as |blogpost| }}
<div class="row mb-4 blogpost">
  <div class="col-md-5">
    <h2>
      <a href="/editpost/{{blogpost.id}}">{{blogpost.title}} {{format_date blogpost.date_created}}</a>
    </h2>
  </div>
</div>
{{/each}}
   <a href="/createpost">+ New Post</a>
